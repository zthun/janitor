{
  "name": "@zthun/vscode-lint-janitor",
  "displayName": "Lint Janitor",
  "description": "Style checks with tools for web projects using common rules.",
  "version": "12.0.0",
  "private": true,
  "engines": {
    "vscode": "^1.58.0"
  },
  "categories": [
    "Linters"
  ],
  "activationEvents": [
    "onCommand:lint-janitor.helloWorld"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "lint-janitor.helloWorld",
        "title": "Lint Janitor Hello"
      }
    ]
  },
  "scripts": {
    "_build": "tsc -p tsconfig.bin.json && typedoc"
  },
  "dependencies": {
    "@zthun/lint-janitor": "^12.0.0"
  }
}
